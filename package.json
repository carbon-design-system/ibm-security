{
  "name": "@carbon/ibm-security",
  "version": "1.2.1-prerelease.4",
  "description": "Carbon for IBM Security",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "homepage": "https://github.com/carbon-design-system/ibm-security/",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    ".storybook/addons/**"
  ],
  "scripts": {
    "build": "npm-run-all -s build:pre build:main build:post",
    "build:pre": "run-s build:pre:*",
    "build:pre:clean": "rm -rf css es lib macros scss storybook-static",
    "build:pre:grid": "css-gridish",
    "build:pre:macros": "babel macros_src -d macros --ignore **/*.template.js --copy-files",
    "build:main": "run-p build:main:*",
    "build:main:cjs": "babel src -d lib --ignore **/_tests_/**,**/__tests__/**,**/*.spec.js,**/*.stories.js,src/globals/scss-exports/index.js",
    "build:main:es": "BABEL_ENV=es babel src -d es --ignore **/_tests_/**,**/__tests__/**,**/*.spec.js,**/*.stories.js,src/globals/scss-exports/index.js",
    "build:main:styles": "node scripts/styles.js",
    "build:main:copy-assets": "node scripts/copyAssets.js",
    "build:main:scss": "gulp copy:scss",
    "build:main:scss-exports": "rollup -c rollup.config.js",
    "build:post": "run-p build:post:*",
    "build:post:storybook": "build-storybook -s public",
    "build:post:clean-css": "cleancss -O2 -o css/index.min.css css/index.css",
    "format": "yarn format:precommit '**/*.{js,scss}'",
    "format:precommit": "prettier --ignore-path .gitignore --write",
    "lint:js": "yarn lint:js:precommit .",
    "lint:js:precommit": "eslint --ignore-path .gitignore --fix",
    "lint:scss": "yarn lint:scss:precommit '**/*.scss'",
    "lint:scss:precommit": "stylelint --fix --ignore-path .gitignore",
    "semantic-release": "semantic-release",
    "start": "yarn build:pre:grid && start-storybook -p 3000 -s public",
    "test": "jest spec -ci",
    "test:precommit": "jest --bail --passWithNoTests --testMatch src/**/*.spec.js --findRelatedTests",
    "test:imports": "scripts/checkImports.sh",
    "test:ssr": "node scripts/ssr"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@carbon/colors": "^10.3.0",
    "@carbon/grid": "^10.6.0",
    "@carbon/icons-react": "^10.6.0",
    "@carbon/import-once": "^10.3.0",
    "@carbon/layout": "^10.5.0",
    "@carbon/themes": "^10.6.1",
    "@carbon/type": "^10.5.1",
    "carbon-components": "^10.6.1",
    "carbon-components-react": "^7.6.1",
    "classnames": "^2.2.5",
    "downshift": "^1.31.14",
    "flatpickr": "^4.6.1",
    "focus-trap-react": "^6.0.0",
    "invariant": "^2.2.3",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-truncate": "^2.4.0",
    "recompose": "0.30.0",
    "reduced-utils": "0.1.2",
    "throttle-debounce": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
