{
  "name": "@carbon/ibm-security",
  "version": "1.45.0-prerelease.3",
  "description": "Carbon for IBM Security",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "homepage": "https://github.com/carbon-design-system/ibm-security/",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    ".storybook/addons/**"
  ],
  "scripts": {
    "audit": "node scripts/audit",
    "build": "run-s build:*",
    "build:pre": "run-s build:pre:*",
    "build:pre:clean": "rm -rf css es lib scss storybook-static",
    "build:main": "run-p build:main:*",
    "build:main:copy-assets": "node scripts/copyAssets.js",
    "build:main:cjs": "babel src -d lib --ignore '**/_mocks_/**','**/__tests__/**','**/*-story.js','src/globals/scss-exports'",
    "build:main:es": "BABEL_ENV=es babel src -d es --ignore '**/_mocks_/**','**/__tests__/**','**/*-story.js','src/globals/scss-exports'",
    "build:main:scss": "gulp copy:scss",
    "build:main:scss-exports": "rollup -c rollup.config.js",
    "build:main:styles": "node scripts/styles.js",
    "build:post": "run-p build:post:*",
    "build:post:clean-css": "cleancss -O2 -o css/index.min.css css/index.css",
    "build:post:storybook": "build-storybook -s public",
    "docs": "node scripts/themes && git add docs/themes/themes.md",
    "format": "yarn precommit:format '**/*.{js, json, html, md, mdx, scss}'",
    "jest": "jest",
    "jest:watch": "yarn jest --watch",
    "lint:js": "yarn precommit:lint:js .",
    "lint:scss": "yarn precommit:lint:scss '**/*.scss'",
    "lint-staged": "lint-staged",
    "postinstall": "carbon-telemetry collect --install",
    "precommit:format": "prettier --ignore-path .gitignore --write",
    "precommit:lint:js": "eslint --ignore-path .gitignore --fix",
    "precommit:lint:scss": "stylelint --fix --ignore-path .gitignore --ip '**/css-gridish/**'",
    "precommit:test": "yarn jest -b --passWithNoTests --testMatch src/**/*.spec.js --findRelatedTests",
    "semantic-release": "semantic-release",
    "start": "start-storybook -p 3000 -s public",
    "test": "run-p test:*",
    "test:js": "yarn jest --ci -i",
    "test:imports": "scripts/checkImports.sh",
    "test:scss": "bundler check 'src/**/*.scss' -i '**/css-gridish/**'",
    "test:ssr": "node scripts/ssr"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.32.8"
  },
