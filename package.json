    "@carbon/telemetry": "^0.0.0-alpha.6",
    "@carbon/themes": "10.40.0",
    "@carbon/type": "10.33.0",
    "carbon-components": "10.41.0",
    "carbon-components-react": "7.41.0",
    "carbon-icons": "7.0.7",
    "classnames": "^2.3.1",
