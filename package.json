    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/git": "^7.1.0-beta.3",
    "@storybook/addon-a11y": "^5.2.0",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-centered": "^5.2.0",
    "@storybook/addon-info": "^5.2.0",
    "@storybook/addon-knobs": "^5.2.0",
    "@storybook/addon-options": "^5.2.0",
    "@storybook/addon-storysource": "^5.2.0",
    "@storybook/addon-viewport": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "@storybook/source-loader": "^5.2.0",
    "@storybook/theming": "^5.2.0",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-ramda": "^2.0.0",
    "clean-css-cli": "^4.1.11",
    "css-gridish": "^2.0.0",
    "css-loader": "^3.0.0",
    "data-uri.macro": "^1.0.0",
    "del": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "fast-sass-loader": "^1.4.7",
    "fs-extra": "^7.0.1",
    "globby": "^9.1.0",
    "gulp": "^4.0.0",
    "handlebars": "^4.0.11",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-canvas-mock": "^2.1.0",
    "js-beautify": "^1.8.9",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.13",
    "postcss-loader": "^3.0.0",
    "postcss-node-sass": "^2.1.7",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.16.4",
    "prismjs": "^1.15.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-syntax-highlighter": "^10.2.0",
    "rollup": "^0.66.5",
    "rollup-plugin-babel": "^4.3.1",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-postcss": "^1.6.2",
    "semantic-release": "^16.0.0-beta.22",
    "style-loader": "^0.23.1",
    "stylelint": "^9.1.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.0",
    "stylelint-scss": "^3.5.2",
    "yarn": "^1.16.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "browserslist": [
    "> 1%",
    "not IE 11"
  ],
  "files": [
    "css",
    "es",
    "lib",
    "dist",
    "scss",
    "storybook-static",
    "macros"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/../coverage",
    "rootDir": "src",
    "setupFiles": [
      "<rootDir>/../config/jest",
      "jest-canvas-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost/"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:carbon-design-system/ibm-security.git"
  }
}
